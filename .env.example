# WOTA Database (Spotter database - used by Prisma)
# Format: mysql://USER:PASSWORD@HOST:PORT/DATABASE
WOTA_DATABASE_URL="mysql://wota_spotter:password@localhost:3306/wota"

# CMS Database (Main WOTA website database)
# Format: mysql://USER:PASSWORD@HOST:PORT/DATABASE
CMS_DATABASE_URL="mysql://username:password@localhost:3306/wota_cms"

# API Server Port (optional, defaults to 3001)
PORT=3001

# Session Secret (change in production)
SESSION_SECRET=your-secret-key-here-change-in-production